    "build": "npm run pre:build && astro build && npm run post:build",
    "post:build": "npm run format",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier -w \"./**/*.{html,css,js,ts,mdx,json,astro,vue}\"",
    "format:check": "prettier --check \"./**/*.{html,css,js,ts,mdx,json,astro,vue}\"",
    "lint": "eslint --fix .",
    "check": "astro check",
    "prepare": "husky install"
  },
  "dependencies": {
    "@astrojs/check": "^0.7.0",
    "@astrojs/mdx": "^3.0.0",
    "@astrojs/sitemap": "3.1.4",
    "@astrojs/vue": "4.2.0",
    "@astrolib/seo": "^0.6.0",
    "@carbon/icons": "^11.41.0",
    "@carbon/icons-vue": "^10.83.1",
    "@hcaptcha/vue3-hcaptcha": "^1.3.0",
    "@netlify/functions": "^2.6.3",
    "@sendgrid/mail": "^8.1.3",
    "astro-i18next": "^1.0.0-beta.21",
    "i18next": "^22.4.10",
    "i18next-vue": "^2.1.1",
    "sharp": "^0.33.3",
    "typescript": "^5.4.5",
    "vue": "^3.4.27"
  },
  "devDependencies": {
    "@hcaptcha/types": "^1.0.3",
    "astro": "^4.8.2",
    "eslint": "^9.2.0",
