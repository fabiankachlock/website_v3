{
  "name": "website_v3",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "start:server": "npm run build && node ./src/server/main.mjs",
    "generate": "astro-i18next generate && npm run format",
